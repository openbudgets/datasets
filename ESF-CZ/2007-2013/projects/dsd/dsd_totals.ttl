@prefix skos:           <http://www.w3.org/2004/02/skos/core#> .

# ----- DSD -----

<http://data.openbudgets.eu/ontology/dsd/esf-czech-projects/totals> a qb:DataStructureDefinition ;
  rdfs:label "DSD of Czech ESF projects - total amounts"@en ;
  rdfs:comment "This dataset contains total amounts of funding received by the Czech ESF projects. The amounts include both the funding from the EU and the Czech Republic, as well as possibly the partner's contribution."@en ;
  qb:component [ qb:dimension obeu-dimension:operationCharacter ;
                 qb:componentAttachment qb:DataSet ;
                 qb:order 1 ],
               [ qb:dimension obeu-dimension:paymentPhase ;
                 qb:order 2 ],
               [ qb:dimension esf-dimension:operationalProgramme ;
                 qb:componentAttachment qb:Slice ;
                 qb:order 3 ],
               [ qb:dimension obeu-dimension:partner ;
                 qb:componentAttachment qb:Slice ;
                 qb:order 4 ],
               [ qb:dimension obeu-dimension:project ;
                 qb:componentAttachment qb:Slice ;
                 qb:order 5 ],
               [ qb:dimension obeu-dimension:date ;
                 qb:componentAttachment qb:Slice ;
                 qb:order 6 ],
               [ qb:measure obeu-measure:amount ;
                 qb:order 7 ],
               [ qb:attribute obeu-attribute:currency ;
                 qb:componentRequired true ;
                 qb:componentAttachment qb:DataSet ;
                 qb:order 8 ] .
