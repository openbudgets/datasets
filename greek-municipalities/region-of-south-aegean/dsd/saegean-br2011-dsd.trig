@prefix saegean-br2011-dimension:  <http://data.openbudgets.eu/ontology/dsd/budget-saegean-revenue-2011/dimension/> .
@prefix saegean-br2011:           <http://data.openbudgets.eu/resource/dataset/budget-saegean-revenue-2011/> .
@prefix saegean-br2011-dsd-slice: <http://data.openbudgets.eu/ontology/dsd/budget-saegean-revenue-2011/slice/> .
@prefix saegean-2011-dimension: <http://data.openbudgets.eu/ontology/dsd/region-of-saegean-2011/dimension/> .
@prefix gr-regions-dimension: <http://data.openbudgets.eu/ontology/dsd/greek-regions/dimension/> .
@prefix saegean-dimension: <http://data.openbudgets.eu/ontology/dsd/region-of-saegean/dimension/> .
@prefix gr-dimension: <http://data.openbudgets.eu/ontology/dsd/greek-municipalities/dimension/> .
@prefix gr-budgetPhase: <http://data.openbudgets.eu/resource/dataset/greek-municipalities/codelist/budget-phase> .
@prefix obeu:             <http://data.openbudgets.eu/ontology/> .
@prefix obeu-attribute:   <http://data.openbudgets.eu/ontology/dsd/attribute/> .
@prefix obeu-dimension:   <http://data.openbudgets.eu/ontology/dsd/dimension/>.
@prefix obeu-measure:     <http://data.openbudgets.eu/ontology/dsd/measure/> .
@prefix obeu-budgetphase: <http://data.openbudgets.eu/resource/codelist/budget-phase/> .
@prefix obeu-codelist:    <http://data.openbudgets.eu/resource/codelist/> .
@prefix qb:               <http://purl.org/linked-data/cube#> .
@prefix rdf:              <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:             <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:             <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:              <http://www.w3.org/2001/XMLSchema#> .
<http://data.openbudgets.eu/ontology/dsd/saegean-br2011-dsd> {
# -----DSD-specific namespaces

# -----Saegean Classifications specific namespaces

# -----OpenBudgets.eu namespaces -----

# -----Generic namespaces ------


# DSD 
<http://data.openbudgets.eu/ontology/dsd/budget-saegean-revenue-2011> a 
  qb:DataStructureDefinition ;
	rdfs:label "Data structure definition for the budget of Saegean Municipality"@en ;
  qb:component
	[ qb:dimension obeu-dimension:organization ;
		qb:componentAttachment qb:DataSet ],
	[ qb:dimension obeu-dimension:operationCharacter ;
		qb:componentAttachment qb:DataSet ],
	[ qb:dimension obeu-dimension:fiscalYear;
		qb:componentAttachment qb:DataSet ],
	[ qb:dimension gr-regions-dimension:administrativeClassification ;
             qb:componentAttachment qb:Slice ],
	[ qb:dimension saegean-2011-dimension:economicClassification ;
                qb:componentAttachment qb:Slice],
	[ qb:dimension saegean-br2011-dimension:functionalClassification;
		qb:componentAttachment qb:Slice ],
	[ qb:dimension saegean-dimension:Classification;
		qb:componentAttachment qb:Slice ],
	[ qb:dimension gr-dimension:budgetPhase], 
	[ qb:attribute obeu-attribute:currency ;
	  qb:componentRequired "true"^^xsd:boolean ;
	  qb:componentAttachment qb:DataSet ],
	[ qb:measure obeu-measure:amount ] ;
	
	qb:sliceKey  saegean-br2011-dsd-slice:slice-by-functionalCode.
	
# Slice key definition
saegean-br2011-dsd-slice:slice-by-functionalCode a qb:SliceKey ;
	rdfs:label "slice by functional code"@en ;
	rdfs:comment "Slice grouping observations of the same functional code for all budget phases."@en ;
	qb:componentProperty saegean-br2011-dimension:functionalClassification ;
	qb:componentProperty gr-regions-dimension:administrativeClassification ;
	qb:componentProperty saegean-dimension:Classification ;
	qb:componentProperty saegean-2011-dimension:economicClassification .

#dimension definition	
saegean-2011-dimension:economicClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "KA of revenue"@en ;
	rdfs:comment "Budget revenue codes (KAE) for greek municipalities"@en ;
	rdfs:subPropertyOf obeu-dimension:economicClassification ;
	qb:codeList obeu-codelist:saegean-kae-esoda-2011 ;
	rdfs:range skos:Concept ;
	rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/region-of-saegean-2011> . 

gr-regions-dimension:administrativeClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "Municipality Office"@en ;
	rdfs:comment "The organization unit (office) of the municipality that is responsible for the budget amount."@en ;
	rdfs:subPropertyOf obeu-dimension:administrativeClassification ;
	qb:codeList obeu-codelist:greek-regions-administrations ;
	rdfs:range skos:Concept ;
	rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/greek-regions> .
	
saegean-br2011-dimension:functionalClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "Municipality Office - KAE Programme"@en ;
	rdfs:comment "Specific function of expenditure budget per office for Municipality of Saegean revenue Budget of year 2011"@en ;
	rdfs:subPropertyOf obeu-dimension:functionalClassification ;
	qb:codeList saegean-br2011:codelist ;
	rdfs:range skos:Concept ;
	rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/budget-saegean-revenue-2011> .
	

	saegean-dimension:Classification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "Regional Unit"@en ;
	rdfs:comment "The regional unit of the region."@en ;
	rdfs:subPropertyOf obeu-dimension:classification ;
	qb:codeList obeu-codelist:sAegean-regional-units ;
	rdfs:range skos:Concept ;
	rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/region-of-saegean> .
}
