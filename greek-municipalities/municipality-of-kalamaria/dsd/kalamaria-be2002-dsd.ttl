# -----DSD-specific namespaces
@prefix kalamaria-be2002-dimension:  <http://data.openbudgets.eu/ontology/dsd/budget-kalamaria-expenditure-2002/dimension/> .
@prefix kalamaria-be2002:           <http://data.openbudgets.eu/resource/dataset/budget-kalamaria-expenditure-2002/> .
@prefix kalamaria-be2002-dsd-slice: <http://data.openbudgets.eu/ontology/dsd/budget-kalamaria-expenditure-2002/slice/> .

# -----Kalamaria Classifications specific namespaces
@prefix kalamaria-2002-dimension: <http://data.openbudgets.eu/ontology/dsd/municipality-of-kalamaria-2002/dimension/> .
@prefix kalamaria-dimension: <http://data.openbudgets.eu/ontology/dsd/municipality-of-kalamaria/dimension/> .

@prefix gr-dimension: <http://data.openbudgets.eu/ontology/dsd/greek-municipalities/dimension/> .
@prefix gr-budgetPhase: <http://data.openbudgets.eu/resource/dataset/greek-municipalities/codelist/budget-phase> .

# -----OpenBudgets.eu namespaces -----
@prefix obeu:             <http://data.openbudgets.eu/ontology/> .
@prefix obeu-attribute:   <http://data.openbudgets.eu/ontology/dsd/attribute/> .
@prefix obeu-dimension:   <http://data.openbudgets.eu/ontology/dsd/dimension/>.
@prefix obeu-measure:     <http://data.openbudgets.eu/ontology/dsd/measure/> .
@prefix obeu-budgetphase: <http://data.openbudgets.eu/resource/codelist/budget-phase/> .
@prefix obeu-codelist:    <http://data.openbudgets.eu/resource/codelist/> .

# -----Generic namespaces ------
@prefix qb:               <http://purl.org/linked-data/cube#> .
@prefix rdf:              <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:             <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:             <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:              <http://www.w3.org/2001/XMLSchema#> .


# DSD 
<http://data.openbudgets.eu/ontology/dsd/budget-kalamaria-expenditure-2002> a 
  qb:DataStructureDefinition ;
	rdfs:label "Data structure definition for the budget of Kalamaria Municipality"@en ;
  qb:component
	[ qb:dimension obeu-dimension:organization ;
		qb:componentAttachment qb:DataSet ],
	[ qb:dimension obeu-dimension:operationCharacter ;
		qb:componentAttachment qb:DataSet ],
	[ qb:dimension obeu-dimension:fiscalYear;
		qb:componentAttachment qb:DataSet ],
	[ qb:dimension kalamaria-dimension:administrativeClassification ;
             qb:componentAttachment qb:Slice ],
	[ qb:dimension kalamaria-2002-dimension:economicClassification ;
                qb:componentAttachment qb:Slice],
	[ qb:dimension kalamaria-be2002-dimension:functionalClassification;
		qb:componentAttachment qb:Slice ],
	[ qb:dimension gr-dimension:budgetPhase], 
	[ qb:attribute obeu-attribute:currency ;
	  qb:componentRequired "true"^^xsd:boolean ;
	  qb:componentAttachment qb:DataSet ],
	[ qb:measure obeu-measure:amount ] ;
	
	qb:sliceKey  kalamaria-be2002-dsd-slice:slice-by-functionalCode.
	
# Slice key definition
kalamaria-be2002-dsd-slice:slice-by-functionalCode a qb:SliceKey ;
	rdfs:label "slice by functional code"@en ;
	rdfs:comment "Slice grouping observations of the same functional code for all budget phases."@en ;
	qb:componentProperty kalamaria-be2002-dimension:functionalClassification ;
	qb:componentProperty kalamaria-dimension:administrativeClassification ;
	qb:componentProperty kalamaria-2002-dimension:economicClassification .

#dimension definition	
kalamaria-2002-dimension:economicClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "KA of expenditure"@en ;
	rdfs:comment "Budget expenditure codes (KAE) for greek municipalities"@en ;
	rdfs:subPropertyOf obeu-dimension:economicClassification ;
	qb:codeList obeu-codelist:kalamaria-kae-eksoda-2002 ;
	rdfs:range skos:Concept ;
	rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/municipality-of-kalamaria-2002> . 

kalamaria-dimension:administrativeClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "Municipality Office"@en ;
	rdfs:comment "The organization unit (office) of the municipality that is responsible for the budget amount."@en ;
	rdfs:subPropertyOf obeu-dimension:administrativeClassification ;
	qb:codeList obeu-codelist:kalamaria-administration-2004 ;
	rdfs:range skos:Concept ;
	rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/municipality-of-kalamaria> .
	
kalamaria-be2002-dimension:functionalClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "Municipality Office - KAE Programme"@en ;
	rdfs:comment "Specific function of expenditure budget per office for Municipality of Kalamaria expenditure Budget of year 2002"@en ;
	rdfs:subPropertyOf obeu-dimension:functionalClassification ;
	qb:codeList kalamaria-be2002:codelist ;
	rdfs:range skos:Concept ;
	rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/budget-kalamaria-expenditure-2002> .
	

