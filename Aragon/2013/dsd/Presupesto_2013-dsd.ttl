# ----- DSD -----
# TO BE CONTINUED FOR ARAGON DATASET.

<http://example.openbudgets.eu/ontology/dsd/ESIF_2014-2020> a qb:DataStructureDefinition ;
  rdfs:label "Data structure definition for the European Structural and Investment Funds of the years 2014-2020"@en ;  
  qb:component [ qb:dimension obeu-dimension:budgetaryUnit ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:budgetPhase ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:operationCharacter ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:fiscalPeriod ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:administrativeClassification ],
               [ qb:dimension esif-dimension:fund ],
               [ qb:dimension esif-dimension:functionalClassification ],
#               [ qb:dimension esif-dimension:priority ],
               [ qb:attribute obeu-attribute:currency ;
                 qb:componentRequired true ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:measure esif-measure:amountEU ],
               [ qb:measure esif-measure:amountNational ],
               [ qb:measure esif-measure:amountTotal ] .


# ----- Component properties -----

esif-dimension:fund a rdf:Property, qb:CodedProperty, qb:DimensionProperty, obeu-dimension:programmeClassification ;
  rdfs:label "European Structural and Investment Fund"@en ;
  rdfs:comment "Different funds of the European Union for sturctual development and investment."@en ;
  rdfs:subPropertyOf obeu-dimension:classification ;
  qb:codeList esif-codelist:funds ;
  rdfs:range esif:Fund ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/ESIF_2014-2020> .

esif-dimension:functionalClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty, obeu-dimension:functionalClassification ;
  rdfs:label "Intention of expenditure"@en ;
  rdfs:comment "Classifies expenditures by the purpose of the funded money."@en ;
  rdfs:subPropertyOf obeu-dimension:classification ;
  qb:codeList esif-codelist:functions ;
  rdfs:range esif:Function ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/ESIF_2014-2020> .

esif-dimension:priority a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
  rdfs:label "Priority"@en ;
  rdfs:comment "?? ."@en ;
  rdfs:subPropertyOf obeu-dimension:classification ;
  qb:codeList esif-codelist:priorities ;
  rdfs:range esif:Priority ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/ESIF_2014-2020> .

esif-measure:amountEU a rdf:Property, qb:DimensionProperty ;
  rdfs:label "amountEU"@en ;
  rdfs:comment "Amount funded by the resp. EU fund."@en ;
  rdfs:subPropertyOf obeu-measure:amount ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/ESIF_2014-2020> .

esif-measure:amountNational a rdf:Property, qb:DimensionProperty ;
  rdfs:label "amountNational"@en ;
  rdfs:comment "National share for the resp. EU fund and project."@en ;
  rdfs:subPropertyOf obeu-measure:amount ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/ESIF_2014-2020> .

esif-measure:amountEU a rdf:Property, qb:DimensionProperty ;
  rdfs:label "amountTotal"@en ;
  rdfs:comment "Sum of the amount funded by the resp. EU fund and the national part."@en ;
  rdfs:subPropertyOf obeu-measure:amount ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/ESIF_2014-2020> .



