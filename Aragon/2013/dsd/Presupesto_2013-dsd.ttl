# ----- DSD -----
# TO BE CONTINUED FOR ARAGON DATASET.

<http://example.openbudgets.eu/ontology/dsd/aragon-budget> a qb:DataStructureDefinition ;
  rdfs:label "Data structure definition for the Aragon dataset for fiscal year 2013"@en ;  
  qb:component [ qb:dimension obeu-dimension:budgetaryUnit ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:budgetPhase ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:operationCharacter ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:fiscalPeriod ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:dimension obeu-dimension:administrativeClassification ], #Identifies the entity responsible for managing the public funds concerned.
               [ qb:dimension obeu-dimension:functionalClassification ], #Purpose of expenditure
               [ qb:dimension obeu-dimension:economicClassification ], #economicClassification
               [ qb:dimension aragon-dimension:RevSourceClassification ], #which organization finance the funds
#               [ qb:dimension esif-dimension:priority ],
               [ qb:attribute obeu-attribute:currency ;
                 qb:componentRequired true ;
                 qb:componentAttachment qb:DataSet ],
               [ qb:measure  obeu-measure:amount] .


# ----- Component properties -----

aragon-dimension:RevSourceClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
  rdfs:label "Funding source organization"@en ;
  rdfs:comment "The organization which provides a funding for specific expenses."@en ;
  rdfs:subPropertyOf obeu-dimension:classification ;
  qb:codeList aragon-codelist:aragon-codelist-fin-rev ;
  rdfs:range aragon-budget:Financing ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/aragon-budget> .


aragon-dimension:ExpSourceClassification a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
  rdfs:label "Organization who receive the funding"@en ;
  rdfs:comment "The organization which receives a funding for specific expenses."@en ;
  rdfs:subPropertyOf obeu-dimension:classification ;
  qb:codeList aragon-codelist:aragon-codelist-fin-exp ;
  rdfs:range aragon-budget:Financing ;
  rdfs:isDefinedBy <http://data.openbudgets.eu/ontology/dsd/aragon-budget> .
  
