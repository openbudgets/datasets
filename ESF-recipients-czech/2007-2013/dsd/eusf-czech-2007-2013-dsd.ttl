				 [ qb:measure lob-measure:status ]
				 .

### Vocabulary ###
				 
lob-dimension:operationalProgramme a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "Operational programme"@en, "Operační program"@cs ;
	qb:codeList lob-codelist:operational-programme ;
	rdfs:subPropertyOf obeu-dimension:programmeClassification ;
	rdfs:isDefinedBy <http://www.strukturalni-fondy.cz/getmedia/a8213c29-3263-4b99-b643-cad46b7d33f4/LIST-OF-BENEFICIARIES_Napoveda_1_a8213c29-3263-4b99-b643-cad46b7d33f4.pdf?ext=.pdf> .
	
lob-dimension:euFund a rdf:Property, qb:CodedProperty, qb:DimensionProperty ;
	rdfs:label "EU Fund"@en, "Fond EU"@cs ;
	qb:codeList lob-codelist:eu-fund ;
	rdfs:subPropertyOf obeu-dimension:programmeClassification ;
	rdfs:isDefinedBy <http://www.strukturalni-fondy.cz/getmedia/a8213c29-3263-4b99-b643-cad46b7d33f4/LIST-OF-BENEFICIARIES_Napoveda_1_a8213c29-3263-4b99-b643-cad46b7d33f4.pdf?ext=.pdf> .

lob-measure:status a rdf:Property, qb:MeasureProperty, qb:CodedProperty ;
	rdfs:label "Status"@en, "Stav"@cs ;
	qb:codeList lob-codelist:status ;
	rdfs:range skos:ConceptScheme ;
	rdfs:isDefinedBy <http://www.strukturalni-fondy.cz/getmedia/a8213c29-3263-4b99-b643-cad46b7d33f4/LIST-OF-BENEFICIARIES_Napoveda_1_a8213c29-3263-4b99-b643-cad46b7d33f4.pdf?ext=.pdf> .	

lob-codelist:status a skos:ConceptScheme ;
	rdfs:label "States of projects"@en, "Stavy projektů"@cs .

lob-status:finalized a skos:Concept ;
	skos:prefLabel "Finalized"@en;
	skos:inScheme lob-codelist:status .
	
lob-status:ongoing a skos:Concept ;
	skos:prefLabel "Ongoing"@en;
	skos:inScheme lob-codelist:status .
	
lob-status:cancelled a skos:Concept ;
	skos:prefLabel "Cancelled"@en;
	skos:inScheme lob-codelist:status .